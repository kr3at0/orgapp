  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" -e ts --exec \"rm -rf dist && tsc -p . && node dist/app.js\"",
    "build": "rm -rf dist && tsc -p .",
    "lint": "tslint -c tslint.json -p ./",
    "knex": "knex --knexfile dist/knexfile.js",
    "db:up": "npm run knex migrate:latest",
